	<style>
		 #dashTable {
            width: 100% height: 85%;
            color: #888888;
        }

        #table1>table {
            width: 100%;
            height: 30%
        }

        #table2>table {
            width: 100%;
            height: 30%
        }

        #table3>table {
            width: 100%;
